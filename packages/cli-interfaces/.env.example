WALLET_MNEMONIC_KEY=your_stacks_mnemonic_here
WALLET_PASSWORD=your_wallet_password

# Legacy contract (for get-messages, post-message)
CONTRACT_ADDRESS=ST1234...
CONTRACT_NAME=your-contract-name

# SIP-10 Token Contracts (optional - defaults provided in config.ts)
STACKS_SBTC_ADDRESS=ST1PQHQKV0RJXZFY1DGX8MNSNYVE3VGZJSRTPGZGM
STACKS_SBTC_NAME=sbtc-token

